				}
			]			
		},		
		"optional_content":{
			"name": "access_content.quoted.double.selinux",
			"patterns": [
				{
					"name": "constant.character.escape.selinux",
					"match": "\\b(optional_policy)\\b"
				}
			]			
		},		
